<query id="updatePlanetMobile" action="update">
	<tables>
		<table name="modules" />
	</tables>
	<columns>
		<column name="use_mobile" var="use_mobile" notnull="notnull" minlength="1" maxlength="1" />
	</columns>
	<conditions>
		<condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull"/>
	</conditions>
</query>
